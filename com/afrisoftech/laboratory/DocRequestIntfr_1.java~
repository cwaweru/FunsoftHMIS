                        while (rse121.next()){
                            
                            glAcc = rse121.getObject(1).toString();
                            
                            java.sql.PreparedStatement pstmt2 = connectDB.prepareStatement("insert into pb_doctors_request values(?,?,?,?,?,?, ?, ?, ?, ?, ?, ?, ?, ?,trim(?))");
                            
