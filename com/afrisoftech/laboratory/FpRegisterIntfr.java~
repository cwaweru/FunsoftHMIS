                                                java.sql.PreparedStatement pstmt2a = connectDB.prepareStatement("insert into credit_acc_slip values(?,?,?,?,?,?, ?, ?)");
                                                
                                                pstmt2a.setString(1,jTextField3.getText());
                                                pstmt2a.setString(2,jTextField6.getText());
                                                pstmt2a.setString(3,jTextField361.getText());
                                                pstmt2a.setString(4,jTextField2.getText());
                                                pstmt2a.setString(5,"00");
                                                pstmt2a.setString(6,slipNo);
                                                pstmt2a.setString(8,"'now'");
                                                pstmt2a.setString(7,user);
                                                pstmt2a.executeUpdate();
                                                jLabel2.setForeground(java.awt.Color.blue);
                                                jLabel2.setText("Insert successful");
                                            }
                                            
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
                if (jCheckBox1.isSelected()){
                    java.sql.PreparedStatement pstmt2 = connectDB.prepareStatement("insert into hp_patient_billing values(?,?,?,?,?,?, ?, ?, ?, ?, ?, ?, ?)");
                    
                    pstmt2.setString(1,jTextField3.getText());
                    pstmt2.setString(2,jTextField6.getText());
                    pstmt2.setString(3,jComboBox1.getSelectedItem().toString());
                    pstmt2.setString(4,jComboBox4.getSelectedItem().toString());
                    pstmt2.setString(5,activity);
                    pstmt2.setString(6,"1");
                    pstmt2.setString(7,jTextField9.getText());
                    pstmt2.setString(8,actNo);
                    pstmt2.setString(9,datePicker4.getDate().toString());
                    pstmt2.setString(10,"");
                    pstmt2.setString(12,"false");
                    pstmt2.setString(11,user);
                    pstmt2.setString(13,mainAcc);
                    pstmt2.executeUpdate();
                }
                java.sql.PreparedStatement pstmt21a = connectDB.prepareStatement("insert into hp_patient_visit values(?,?,?,?,?,?, ?, ?, ?, ?, ?, ?, ?,?,?,?)");
                
                pstmt21a.setString(1,jTextField3.getText());
                pstmt21a.setString(2,jTextField6.getText());
                pstmt21a.setString(3,"OP");
                pstmt21a.setString(4,jComboBox1.getSelectedItem().toString());
                pstmt21a.setString(5,slipNo);
                pstmt21a.setString(6,activity);
                pstmt21a.setString(7,"1");
                pstmt21a.setString(8,mainAcc);
                pstmt21a.setString(9,"0.00");
                pstmt21a.setString(11,datePicker4.getDate().toString());
                pstmt21a.setString(10,"");
                pstmt21a.setString(12,"0");
                pstmt21a.setString(14,user);
                pstmt21a.setString(13,"");
                pstmt21a.setString(15,"'now'");
                pstmt21a.setString(16,"Registration");
                
                pstmt21a.executeUpdate();
                
                try {
                    java.sql.Statement stmtn = connectDB.createStatement();
                    java.sql.ResultSet rsetn = stmtn.executeQuery("select distinct patient_no,oid from hp_patient_register where date is not null ORDER BY oid DESC LIMIT 1");
                    while (rsetn.next()){
                        jTextField171.setText(rsetn.getObject(1).toString());
                    }
                    rsetn.close();
                    stmtn.close();
                    
                    
                } catch (java.sql.SQLException sqe) {
                    sqe.printStackTrace();
                    System.out.println("selection not successful");
                }
                
                connectDB.commit();
                connectDB.setAutoCommit(true);
                
                
                jButton301.setEnabled(false);
                jButton3.setEnabled(false);
                jTextField4.setText("");
                jTextField1.setText("");
                //                jTextField14.setText("");
                jTextField5.setText("");
                jTextField7.setText("");
                jTextField8.setText("");
                jTextField5821.setText("");
                jPanel2.invalidate();
                jPanel5.invalidate();
                jPanel2.validate();
                jPanel5.validate();
            }catch(java.sql.SQLException sq){
                
                try {
                    connectDB.rollback();
                }catch (java.sql.SQLException sql){
                    javax.swing.JOptionPane.showMessageDialog(this,sql.getMessage(),"Error Message!",javax.swing.JOptionPane.ERROR_MESSAGE);
                }
                System.out.println(sq.getMessage());
                javax.swing.JOptionPane.showMessageDialog(this,sq.getMessage(), "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
                
            }
            
        } catch(java.lang.Exception ex) {
            System.out.println(ex.getMessage());
            javax.swing.JOptionPane.showMessageDialog(this, "TRANSACTION ERROR : Please double check your entries.", "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
            
            
        }
        
