    private void jSearchTable21MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jSearchTable21MouseClicked
       
        jTextField91.setText(jSearchTable21.getValueAt(jSearchTable21.getSelectedRow(), 0).toString());
        jSearchDialog21.dispose();
          int i = 0;
        try {
            
            
            java.sql.Statement stmt1 = connectDB.createStatement();
            java.sql.ResultSet rset1 = stmt1.executeQuery("select service,amount,requisition_no,bed_no,time_due,dosage from pb_doctors_request where inv_no = '"+jTextField91.getText()+"'");
            while (rset1.next()){
                jTable11.setValueAt(rset1.getObject(1),i,0);
                jTable11.setValueAt(rset1.getObject(2),i,1);
                jTable11.setValueAt(rset1.getObject(3),i,2);
                jTable11.setValueAt(rset1.getObject(4),i,3);
                 jTable11.setValueAt(rset1.getObject(5),i,4);
                  jTable11.setValueAt(rset1.getObject(6),i,5);
                   i++;
            }
           
        } catch (java.sql.SQLException sqe) {
            sqe.printStackTrace();
            //  System.out.println("Insert not successful");
        }
        
        
          // Add your handling code here:
    }//GEN-LAST:event_jSearchTable21MouseClicked

    private void jTextField1131CaretUpdate(javax.swing.event.CaretEvent evt) {//GEN-FIRST:event_jTextField1131CaretUpdate
               if(this.jCheckBox2.isSelected()){
            
            if(jTextField1131.getCaretPosition() < 1){
                
                System.out.println("Nothing");
            }else{
                jSearchTable21.setModel(com.afrisoftech.dbadmin.TableModel.createTableVectors(connectDB,"select distinct inv_no,patient_no,patient_name FROM pb_doctors_request WHERE inv_no ILIKE '%"+jTextField1131.getText()+"%' and revenue_code ilike '%pharm%' and patient_no='"+jTextField9.getText()+"'  order by inv_no"));
                //  select product as service_type,selling_price as price,gl_code FROM st_stock_prices WHERE product ILIKE '%"+jTextField11.getText()+"%' AND department ilike '"+jComboBox1.getSelectedItem()+"%' order by service_type
                
                
                jSearchScrollPane21.setViewportView(jSearchTable21);
                
                
            }
        }else{
        }// Add your handling code here:
    }//GEN-LAST:event_jTextField1131CaretUpdate

