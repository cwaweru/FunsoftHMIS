    public void run() {
        
        while(getList) {
            
            runGetListThread();
            
            try {
                
                Thread.currentThread().sleep(100000);
                
            } catch(java.lang.InterruptedException IntExec){
                
                javax.swing.JOptionPane.showMessageDialog(this, IntExec.getMessage(), "Interruption Error!", javax.swing.JOptionPane.ERROR_MESSAGE);
                
            }
            
            // getList = false;
            
        }
        
    }
    
    public void runGetListThread() {
        
        int j = 0;
        int i = 0;
        int n = 0;
        int patNo = 0;
        String mssg = null;
        String from = null;
         String tyme = null;
     java.util.Date labNo1 = null;
      //   String labNo1 = null;
        try {
             java.sql.Statement stmtTable113 = connectDB.createStatement();
            
            java.sql.ResultSet rsetTable113 = stmtTable113.executeQuery("select current_time::time(0) ");
         
              while (rsetTable113.next()) {
                labNo1 = rsetTable113.getTime(1);
                        System.out.println("time is "+labNo1);
                         this.jLabel4.setText("Its "+labNo1);
          //  this.jLabel1.setForeground(new java.awt.Color(255,0,51));
            }   
           
            
         
            
        
        } catch(java.sql.SQLException sqlExec) {
            
            sqlExec.printStackTrace();
            
            javax.swing.JOptionPane.showMessageDialog(this, sqlExec.getMessage());
            
        }
      if(labNo1.getTime() >1 && labNo1.getHours()<10){
          //  this.jLabel711.gsetText("From,'"+from+"' ,'"+mssg+"'");
            this.jLabel1.setText("Good Morning!");
            this.jLabel1.setForeground(new java.awt.Color(0,0,255));
             System.out.println("t1 is "+labNo1);
            
      }else{
           if(labNo1.getTime() >10 && labNo1.getHours()<11){
          //  this.jLabel711.gsetText("From,'"+from+"' ,'"+mssg+"'");
            this.jLabel1.setText("Good Morning! It's Tea Time");
            this.jLabel1.setForeground(new java.awt.Color(255,0,51));
            System.out.println("t2 is "+labNo1);
            }else{
           if(labNo1.getTime() >11 && labNo1.getHours()<12){
          //  this.jLabel711.gsetText("From,'"+from+"' ,'"+mssg+"'");
            this.jLabel1.setText("Nice Tea! Wellcome Back");
            this.jLabel1.setForeground(new java.awt.Color(255,0,51));
            System.out.println("t21 is "+labNo1);
        }else{
            if(labNo1.getTime() >12 && labNo1.getHours()<13){
          //  this.jLabel711.gsetText("From,'"+from+"' ,'"+mssg+"'");
            this.jLabel1.setText("Good AfterNoon! It's Past Midday");
            this.jLabel1.setForeground(new java.awt.Color(255,0,51));
             System.out.println("t3 is "+labNo1);
            
               }else{
       
           if(labNo1.getTime() >13 && labNo1.getHours()<14){
          //  this.jLabel711.gsetText("From,'"+from+"' ,'"+mssg+"'");
            this.jLabel1.setText("Good AfterNoon! It's Lunch Break");
            this.jLabel1.setForeground(new java.awt.Color(255,0,51));
             System.out.println("t31 is "+labNo1);
            
               }else{
            
          
           if(labNo1.getTime() >14 && labNo1.getHours()<16){
          //  this.jLabel711.gsetText("From,'"+from+"' ,'"+mssg+"'");
            this.jLabel1.setText("Good Afternoon! Wellcome Back");
            this.jLabel1.setForeground(new java.awt.Color(255,0,255));
           
           
       
           
       
            
            }else{
            
          
           if(labNo1.getTime() >16 && labNo1.getHours()<17){
          //  this.jLabel711.gsetText("From,'"+from+"' ,'"+mssg+"'");
            this.jLabel1.setText("Good Afternoon! Enjoy Your 4 O'cklock Tea");
            this.jLabel1.setForeground(new java.awt.Color(255,0,255));
           
            System.out.println("t4 is "+labNo1);
       
           
        }else{
            
          
           if(labNo1.getTime() >17 && labNo1.getHours()<24){
          //  this.jLabel711.gsetText("From,'"+from+"' ,'"+mssg+"'");
            this.jLabel1.setText("Have A Good Evening!");
            this.jLabel1.setForeground(new java.awt.Color(255,0,255));
            System.out.println("t5 is "+labNo1);
           
        }
        }
   
    }
     
    }
