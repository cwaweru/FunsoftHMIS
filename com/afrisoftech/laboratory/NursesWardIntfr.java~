    private void jSearchTable2MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jSearchTable2MouseClicked
        int j = 0;
        jTextField7.setText(java.lang.String.valueOf(0));
        jTextField8.setText(java.lang.String.valueOf(0));
        jTextField10.setText(java.lang.String.valueOf(0));
        
        java.text.DateFormat df = java.text.DateFormat.getDateInstance();
        
        java.text.SimpleDateFormat sdf = (java.text.SimpleDateFormat)df;
        
        sdf.applyPattern("yyyy-MM-dd");
        
       
        jTextField2.setText(jSearchTable2.getValueAt(jSearchTable2.getSelectedRow(), 1).toString());
        jTextField91.setText(jSearchTable2.getValueAt(jSearchTable2.getSelectedRow(), 0).toString());
        jTextField5.setText(jSearchTable2.getValueAt(jSearchTable2.getSelectedRow(), 3).toString());
        
        
        try {
            java.sql.Statement stmt = connectDB.createStatement();
            java.sql.ResultSet rset = stmt.executeQuery("select ward,bed_no,ward_code,date_admitted from hp_admission where admission_no ='"+jTextField91.getText()+"'");
            while (rset.next()){
                //   jTextField2.setText(rset.getObject(1).toString());
                jTextField4.setText(rset.getObject(1).toString());
                jTextField6.setText(rset.getObject(2).toString());
                //                jTextField61.setText(rset.getObject(4).toString());
                // jTextField5.setText(rset.getObject(3).toString());
            }
            
            java.sql.Statement stmt11 = connectDB.createStatement();
            java.sql.ResultSet rset11 = stmt11.executeQuery("select count(patient_no) from hp_patient_card where patient_no ='"+jTextField91.getText()+"' AND visit_id = '"+jTextField5.getText()+"'");
            while (rset11.next()){
                j = rset11.getInt(1);
                
            }
            if (j > 0){
                
                java.sql.Statement stmt1 = connectDB.createStatement();
                java.sql.ResultSet rset1 = stmt1.executeQuery("select sum(debit - credit) from hp_patient_card where patient_no ='"+jTextField91.getText()+"' AND visit_id = '"+jTextField5.getText()+"'");
                while (rset1.next()){
                    jTextField3.setText(rset1.getString(1));
                    
                }
            } else {
                jTextField3.setText("0.00");
            }
        } catch (java.sql.SQLException sqe) {
            sqe.printStackTrace();
            System.out.println("select not successful");
        }
        
        this.jSearchDialog2.dispose();
        // Add your handling code here:
    }//GEN-LAST:event_jSearchTable2MouseClicked
    
