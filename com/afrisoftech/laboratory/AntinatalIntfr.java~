    private void jSearchTable2MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jSearchTable2MouseClicked
        this.jTextField1.setText("");
        this.jTextField10.setText("");
        this.jTextField91.setText("");
        this.jTextField7.setText("");
        this.jTextField11.setText("");
        this.jTextField113.setText("");
        this.jTextField1131.setText("");
        this.jTextField12.setText("");
        this.jTextField13.setText("");
        this.jTextField14.setText("");
        this.jTextField2.setText("");
        this.jTextField3.setText("");
        this.jTextField4.setText("");
        this.jTextField5.setText("");
        this.jTextField6.setText("");
        this.jTextField8.setText("");
        
        this.jTextField9.setText("");
        this.jTextPane1.setText("");
        this.jTextPane11.setText("");
        this.jTextPane2.setText("");
        this.jTextPane21.setText("");
         this.jTextPane3.setText("");
       
        
        int j = 0;
        //   jTextField7.setText(java.lang.String.valueOf(0));
        //        jTextField8.setText(java.lang.String.valueOf(0));
        //        jTextField10.setText(java.lang.String.valueOf(0));
        
        
        jTextField2.setText(jSearchTable2.getValueAt(jSearchTable2.getSelectedRow(), 1).toString());
        jTextField91.setText(jSearchTable2.getValueAt(jSearchTable2.getSelectedRow(), 0).toString());
        jTextField5.setText(jSearchTable2.getValueAt(jSearchTable2.getSelectedRow(), 3).toString());
        
          this.jSearchDialog2.dispose();
        try {
            java.sql.Statement stmt = connectDB.createStatement();
            java.sql.ResultSet rset = stmt.executeQuery("select date_admitted,discharge_date from hp_admission where admission_no ='"+jTextField91.getText()+"' order by discharge_date desc limit 1");
            while (rset.next()){
                //   jTextField2.setText(rset.getObject(1).toString());
                jTextField10.setText(rset.getObject(1).toString());
               // jTextField8.setText(rset.getObject(2).toString());
                //                jTextField61.setText(rset.getObject(4).toString());
                // jTextField5.setText(rset.getObject(3).toString());
            }
            java.sql.Statement stmt2 = connectDB.createStatement();
            java.sql.ResultSet rset2 = stmt2.executeQuery("select discharge_date from hp_patient_discharge where patient_no ='"+jTextField91.getText()+"' order by discharge_date desc limit 1");
            while (rset2.next()){
                //   jTextField2.setText(rset.getObject(1).toString());
               // jTextField10.setText(rset.getObject(1).toString());
                jTextField8.setText(rset2.getObject(1).toString());
                //                jTextField61.setText(rset.getObject(4).toString());
                // jTextField5.setText(rset.getObject(3).toString());
            }
            
        } catch (java.sql.SQLException sqe) {
            sqe.printStackTrace();
            System.out.println("select not successful");
        }
          
          java.text.DateFormat df = java.text.DateFormat.getDateInstance();
        
        java.text.SimpleDateFormat sdf = (java.text.SimpleDateFormat)df;
        
        sdf.applyPattern("yyyy-MM-dd");
        java.text.DateFormat df2 = java.text.DateFormat.getDateInstance();
        
        java.text.SimpleDateFormat sdf2 = (java.text.SimpleDateFormat)df2;
        
        sdf2.applyPattern("yyyy-MM-dd");
          
          
          try {
                 datePicker11.setDate(sdf.parse(jTextField10.getText().toString()));
     
              datePicker12.setDate(sdf2.parse(jTextField8.getText().toString()));
        } catch (java.text.ParseException pe) {
            javax.swing.JOptionPane.showMessageDialog(this, pe.getMessage());
        }
        this.jSearchDialog2.dispose();
        
        // Add your handling code here:
    }//GEN-LAST:event_jSearchTable2MouseClicked
    
