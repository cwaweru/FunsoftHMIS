    private void jTextField1131CaretUpdate(javax.swing.event.CaretEvent evt) {//GEN-FIRST:event_jTextField1131CaretUpdate
        if(jTextField1131.getCaretPosition() < 2){
            
            System.out.println("Nothing");
        }else{
            jSearchTable21.setModel(com.afrisoftech.dbadmin.TableModel.createTableVectors(connectDB,"SELECT disease_name as name from hp_diseases where disease_name ILIKE '"+jTextField1131.getText()+"%' ORDER BY disease_name"));
            
