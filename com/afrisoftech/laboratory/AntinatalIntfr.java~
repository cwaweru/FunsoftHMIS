    }//GEN-LAST:event_jButton5ActionPerformed
    
    private void jButton111ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton111ActionPerformed
        String disno = null;
        String UserName = null;
        try {
            connectDB.setAutoCommit(false);
            java.sql.Statement ps = connectDB.createStatement();
            java.sql.ResultSet rst = ps.executeQuery("select currval('medrep_seq'),current_user");
            while (rst.next()){
                rst.getObject(1).toString();
                
                //disno = rst.getObject(1).toString();
                UserName = rst.getObject(2).toString();
            }
            disno = jTextField14.getText().toString();
            // com.afrisoftech.laboratory.PatientMedPdf policy = new com.afrisoftech.laboratory.PatientMedPdf(connectDB, disno);
            // policy.GatePassPdf(connectDB, disno);
            com.afrisoftech.laboratory.PatientMedPdf policy = new com.afrisoftech.laboratory.PatientMedPdf();
            
            policy.PatientMedPdf(connectDB,jTextField14.getText().toString());
            
        }catch(java.sql.SQLException sq){
            
            try {
                connectDB.rollback();
            }catch (java.sql.SQLException sql){
                javax.swing.JOptionPane.showMessageDialog(this,sql.getMessage(),"Error Message!",javax.swing.JOptionPane.ERROR_MESSAGE);
            }
            System.out.println(sq.getMessage());
            javax.swing.JOptionPane.showMessageDialog(this,sq.getMessage(), "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
            
        }      // Add your handling code here:
    }//GEN-LAST:event_jButton111ActionPerformed
    
    private void jButton11ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton11ActionPerformed
        
        
        
        String disno = null;
        String UserName = null;
        try {
            jButton1ActionPerformed(evt);
            connectDB.setAutoCommit(false);
            // java.sql.Statement ps = connectDB.createStatement();
            // disno = jTextField14.getText().toString();
            // com.afrisoftech.laboratory.PatientMedPdf policy = new com.afrisoftech.laboratory.PatientMedPdf(connectDB, disno);
            // policy.GatePassPdf(connectDB, disno);
            com.afrisoftech.laboratory.PatientMedPdf policy = new com.afrisoftech.laboratory.PatientMedPdf();
            
            policy.PatientMedPdf(connectDB,jTextField14.getText().toString());
            
        }catch(java.sql.SQLException sq){
            
            try {
                connectDB.rollback();
            }catch (java.sql.SQLException sql){
                javax.swing.JOptionPane.showMessageDialog(this,sql.getMessage(),"Error Message!",javax.swing.JOptionPane.ERROR_MESSAGE);
            }
            System.out.println(sq.getMessage());
            javax.swing.JOptionPane.showMessageDialog(this,sq.getMessage(), "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
            
        }        // Add your handling code here:
    }//GEN-LAST:event_jButton11ActionPerformed
    
