    private void jTextField111CaretUpdate(javax.swing.event.CaretEvent evt) {//GEN-FIRST:event_jTextField111CaretUpdate
       if(this.jCheckBox1.isSelected()){
        
        if(jTextField111.getCaretPosition() < 2){
            
            System.out.println("Nothing");
        }else{
            jSearchTable1.setModel(com.afrisoftech.dbadmin.TableModel.createTableVectors(connectDB,"select employee_no,first_name||' '||middle_name||' '||last_name AS name,department,mobile_no from master_file where first_name||' '||middle_name||' '||last_name ILIKE '%"+jTextField111.getText()+"%' ORDER BY first_name||' '||middle_name||' '||last_name"));
            
            
            jSearchTable1.setShowHorizontalLines(false);
            jSearchScrollPane1.setViewportView(jSearchTable1);
            
            
        }
       }else{
         if(jTextField111.getCaretPosition() < 2){
            
            System.out.println("Nothing");
        }else{
            jSearchTable1.setModel(com.afrisoftech.dbadmin.TableModel.createTableVectors(connectDB,"select code,comp AS name,description,phone_no from pb_cont_list where comp ILIKE '%"+jTextField111.getText()+"%' ORDER BY comp"));
            
            
            jSearchTable1.setShowHorizontalLines(false);
            jSearchScrollPane1.setViewportView(jSearchTable1);
            
            
        }  
           
       }// Add your handling code here:
    }//GEN-LAST:event_jTextField111CaretUpdate

