    private void jButton1111ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1111ActionPerformed
        
        //try{
        String Inst = null;
        String transNo1 = null;
        String glType = null;
        String user = null;
        int vid = 0;
        
        String Priority = null;
        String Delv = null;
        
       
       
        
         if(this.jCheckBox7.isSelected()){
            
            Priority = jCheckBox7.getText();
        }else{
            if(this.jCheckBox8.isSelected()){
                
                Priority = jCheckBox8.getText();
            
            }else{
            if(this.jCheckBox12.isSelected()){
                
                Priority = jCheckBox12.getText();
            }
        }
        }
        
        
        
       
  
        
        try {
            connectDB.setAutoCommit(false);
            
            java.sql.Statement stm12 = connectDB.createStatement();
            java.sql.ResultSet rse12 = stm12.executeQuery("select current_user ");
            while (rse12.next()){
                user = rse12.getObject(1).toString();
                
            }
            
            
            
            java.sql.Statement pss = connectDB.createStatement();
            java.sql.ResultSet rss = pss.executeQuery("select nextval('task_no_seq')");
            while (rss.next()){
                transNo1 = rss.getObject(1).toString();
                this.jTextField2.setText(transNo1);
                
            }
            
          for (int i = 0; i < jTable1.getRowCount(); i++){
                if (jTable1.getModel().getValueAt(i,0) != null){
            
            java.sql.PreparedStatement pstmtx = connectDB.prepareStatement("insert into pb_tasks values(?,?,?,?,?,?,?,?,?,?)");
            if (jTextField20.getText().equals("")) {
                            javax.swing.JOptionPane.showMessageDialog(this,"Consignor Name missing", "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
                        } else{
            
            pstmtx.setString(1,jTextField20.getText());
                        }
             
            pstmtx.setObject(2,jTextField161.getText());
                       
            
             pstmtx.setString(3,datePicker2.getDate().toString());
           
            pstmtx.setString(4,jComboBox111.getSelectedItem().toString());
            pstmtx.setString(5,jTextField2.getText());
            
              pstmtx.setString(6,Priority);
              pstmtx.setString(8,jTable1.getValueAt(i,0).toString());
              pstmtx.setString(7,jTextField3.getText());
                  
            
            pstmtx.setString(10,"'now'");
           
            pstmtx.setString(9,user);
                         
                       
            pstmtx.executeUpdate();
            
        }
    }
            connectDB.commit();
            connectDB.setAutoCommit(true);
            //   this.jButton1111.setEnabled(false);
            javax.swing.JOptionPane.showMessageDialog(this, "Insert Done Successfully","Confirmation Message",javax.swing.JOptionPane.INFORMATION_MESSAGE);
            
          //  this.getContentPane().removeAll();
          //  this.initComponents();
            
            //   jButton1111.setEnabled(false);
        }catch(java.sql.SQLException sq){
            
            try {
                connectDB.rollback();
            }catch (java.sql.SQLException sql){
                javax.swing.JOptionPane.showMessageDialog(this,sql.getMessage(),"Error Message!",javax.swing.JOptionPane.ERROR_MESSAGE);
            }
            System.out.println(sq.getMessage());
            javax.swing.JOptionPane.showMessageDialog(this,sq.getMessage(), "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
            
        }
        
        /*} catch(java.lang.Exception ex) {
            System.out.println(ex.getMessage());
            javax.swing.JOptionPane.showMessageDialog(this, "TRANSACTION ERROR : Please double check your entries.", "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
         
         
        }
         */
        // Add your handling code here:
    }//GEN-LAST:event_jButton1111ActionPerformed
    
