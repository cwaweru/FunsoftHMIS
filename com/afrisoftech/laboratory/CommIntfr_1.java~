    private void jTextArea1CaretUpdate(javax.swing.event.CaretEvent evt) {//GEN-FIRST:event_jTextArea1CaretUpdate
          int MessMax =120;
        int NumCount =0;
        NumCount = NumCount + this.jTextArea1.getSelectionEnd(); 
//this.jLabel1.setText(""+NumCount);

if(NumCount<MessMax){
  //  this.jLabel2.setForeground(red);
    //NumCount<=MessMax;
   this.jLabel21.setForeground(new java.awt.Color(255,0,51));
     this.jLabel11.setForeground(new java.awt.Color(255,0,51));
    this.jLabel21.setText(""+NumCount);
     this.jLabel11.setText("1 SMS");
   
}else{
   
    this.jLabel21.setText(""+NumCount); 
         this.jLabel21.setForeground(new java.awt.Color(51,51,255));
         this.jLabel11.setForeground(new java.awt.Color(51,51,255));
  this.jLabel11.setText("2 SMS");
   
   //javax.swing.JOptionPane.showMessageDialog(this, "You are exceeding the No. of characters!","Confirmation Message",javax.swing.JOptionPane.INFORMATION_MESSAGE);
                   
    // this.jLabel2.setForeground(red);
}  // Add your handling code here:
    }//GEN-LAST:event_jTextArea1CaretUpdate

    private void jTextField20ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField20ActionPerformed
        try {
          
            
            java.sql.Statement stm12 = connectDB.createStatement();
            java.sql.ResultSet rse12 = stm12.executeQuery("select current_user ");
            while (rse12.next()){
              
            this.jTextField20.setText(rse12.getObject(1).toString());
            }
             
        }catch(java.sql.SQLException sq){
            
            
            System.out.println(sq.getMessage());
            javax.swing.JOptionPane.showMessageDialog(this,sq.getMessage(), "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
            
        }
