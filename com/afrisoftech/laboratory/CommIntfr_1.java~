    private void hand(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_hand
       timeLabel.setText( new Date().toString() );
    // scrollLabel.setText(jLabel711.getText());
     
    //scrollLabel.setText("HaLloooooooooooooooooo!!!!!!!!!!!!1 ");

		String oldText = scrollLabel.getText();
 
		// Scroll right to left
	String newText = oldText.substring(1) + oldText.substring(0,1);
 
		// Scroll left to right
		//int length = oldText.length();
//		String newText = oldText.substring(length-1, length) + oldText.substring(0, length-1);
 
		scrollLabel.setText( newText );
                  System.out.println("Text "+ newText); // Add your handling code here:
    }//GEN-LAST:event_hand
