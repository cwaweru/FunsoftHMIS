    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
       String Thno =null;
        String user =null;
            try {
            connectDB.setAutoCommit(false);
            java.sql.Statement ps11 = connectDB.createStatement();
            java.sql.ResultSet rst11 = ps11.executeQuery("select nextval('booking_no_seq'),current_user");
            while (rst11.next()){
             
            Thno = rst11.getObject(1).toString();
            user = rst11.getObject(2).toString();
            }
            java.sql.PreparedStatement pstmt = connectDB.prepareStatement("insert into hp_theatre_booking values(?,?,?,?,?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?,?,?)");
                    pstmt.setString(1,jTextField36.getText());
                    pstmt.setString(2,jTextField1.getText());
                    pstmt.setString(4,jTextField361.getText());
                    pstmt.setString(3,jTextField3611.getText());
                    pstmt.setString(5,jTextField21.getText());
                    pstmt.setString(6,jTextField2.getText());
                    pstmt.setString(9,jTextField362.getText());
                    pstmt.setString(7,datePicker1.getDate().toString());
                      pstmt.setString(8,jComboBox11.getSelectedItem().toString());
                    pstmt.setString(10,jComboBox111.getSelectedItem().toString());
                     pstmt.setString(11,jComboBox3.getSelectedItem().toString());
                      pstmt.setString(12,jTextPane1.getText());
                   pstmt.setString(14,"");
                  
                    pstmt.setString(13,"OP");
                      pstmt.setString(15,"'now'");
                   pstmt.setString(16,Thno);
                  pstmt.setString(17,user);
                    pstmt.executeUpdate();
 
            connectDB.commit();
            connectDB.setAutoCommit(true);
            jLabel7.setForeground(java.awt.Color.blue);
            jLabel7.setText("Insert successful");
            this.getContentPane().removeAll();
            this.initComponents();
            //  jComboBox2.setSelectedItem(null);
        }catch(java.sql.SQLException sq){
            
            try {
                connectDB.rollback();
            }catch (java.sql.SQLException sql){
                javax.swing.JOptionPane.showMessageDialog(this,sql.getMessage(),"Error Message!",javax.swing.JOptionPane.ERROR_MESSAGE);
            }
            System.out.println(sq.getMessage());
            javax.swing.JOptionPane.showMessageDialog(this,sq.getMessage(), "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
            
        }   // Add your handling code here:
    }//GEN-LAST:event_jButton1ActionPerformed
    
