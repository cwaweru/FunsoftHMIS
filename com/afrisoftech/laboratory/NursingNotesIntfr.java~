    private void jButton1111ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1111ActionPerformed
        
        
        String user = null;
        String nodetails = null;
        int vid = 0;
        
        
        
        
        try {
            connectDB.setAutoCommit(false);
            
            java.sql.Statement stm12 = connectDB.createStatement();
            java.sql.ResultSet rse12 = stm12.executeQuery("select current_user ");
            while (rse12.next())
                
                user = rse12.getObject(1).toString();
            
            
            
            java.sql.Statement pss = connectDB.createStatement();
            
            
            
            java.sql.PreparedStatement pstmtx = connectDB.prepareStatement("insert into hp_clinical_ip values(?,?,?,?,?,?,?,?,?,?,?,?,?)");
            pstmtx.setString(3,jTextPane1.getText());
            pstmtx.setObject(1,jTextField9.getText());
            pstmtx.setObject(2,jTextField20.getText());
            pstmtx.setObject(4,jTextField3.getText());
            pstmtx.setString(6,jTextField6.getText());
            
            pstmtx.setString(5,jTextField17.getText());
            
            pstmtx.setString(7,datePicker2.getDate().toString());
            pstmtx.setString(8,jTextField5.getText());
            pstmtx.setString(9,jTextField4.getText());
            
            pstmtx.setString(11,user);
            
            pstmtx.setString(10,"'now'");
            
            pstmtx.setObject(12,jTextField2.getText());
            pstmtx.setObject(13,jTextField1.getText());
            
            
            pstmtx.executeUpdate();
            
            
            //  pstmt2.setObject(26,"InPatient");
            
            //  pstmt2.executeUpdate();
            connectDB.commit();
            connectDB.setAutoCommit(true);
            this.jButton1111.setEnabled(false);
            javax.swing.JOptionPane.showMessageDialog(this, "Insert Done Successfully","Confirmation Message",javax.swing.JOptionPane.INFORMATION_MESSAGE);
            // jButton31112ActionPerformed(evt);
            
            //   jButton1111.setEnabled(false);
        }catch(java.sql.SQLException sq){
            
            try {
                connectDB.rollback();
            }catch (java.sql.SQLException sql){
                javax.swing.JOptionPane.showMessageDialog(this,sql.getMessage(),"Error Message!",javax.swing.JOptionPane.ERROR_MESSAGE);
            }
            System.out.println(sq.getMessage());
            javax.swing.JOptionPane.showMessageDialog(this,sq.getMessage(), "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
            
        }
        
        /*} catch(java.lang.Exception ex) {
            System.out.println(ex.getMessage());
            javax.swing.JOptionPane.showMessageDialog(this, "TRANSACTION ERROR : Please double check your entries.", "Error",javax.swing.JOptionPane.ERROR_MESSAGE);
         
         
        }
         */
        // Add your handling code here:
    }//GEN-LAST:event_jButton1111ActionPerformed
    
