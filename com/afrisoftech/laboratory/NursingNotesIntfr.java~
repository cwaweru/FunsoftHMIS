                pstmt2.setString(9,jTextField1211.getText());
                pstmt2.setString(10,jTextField111.getText());
                
                pstmt2.setObject(11,jTextField24.getText());
                
                pstmt2.setObject(12,jTextField8.getText());
                pstmt2.setString(13,jTextField121.getText());
                pstmt2.setString(14,user);
                pstmt2.setString(15,datePicker2.getDate().toString());
                pstmt2.setString(16,"'now'");
                
                pstmt2.setObject(17,jTextField171.getText());
                
                
                pstmt2.executeUpdate();
                
                
                
                
                connectDB.commit();
                connectDB.setAutoCommit(true);
                javax.swing.JOptionPane.showMessageDialog(this,"Insert Successful","Confirmation Message!",javax.swing.JOptionPane.INFORMATION_MESSAGE);
                
                com.afrisoftech.laboratory.RefferalPdf pol = new com.afrisoftech.laboratory.RefferalPdf();
                pol.RefferalPdf(connectDB, jTextField101.getText(),jTextField101.getText());
                
